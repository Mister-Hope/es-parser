{
  "name": "es-parser",
  "version": "0.0.1",
  "description": "A mini es parser",
  "main": "dist/parser.js",
  "types": "dist/parser.d.ts",
  "scripts": {
    "dev": "webpack --watch",
    "build": "webpack",
    "test": "mocha -r ts-node/register test/unit/**/*.ts",
    "test:parser": "ts-node ./test/parser.ts",
    "test:dev": "mocha -r ts-node/register  --watch test/unit/**/*.ts",
    "test:js": "mocha ./test/unit/index.js",
    "parse": "node ./test/parse.js",
    "debug": "mocha -r ts-node/register  --watch test/debug.ts",
    "lint": "prettier --check --write . && eslint --fix --ext .js,.ts .",
    "lint:check": "prettier --check . && eslint --ext .js,.ts .",
    "commit": "npx git-cz"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mister-Hope/es-parser.git"
  },
  "keywords": [
    "javascript",
    "parser",
    "mr-hope"
  ],
  "author": "Mr.Hope",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mister-Hope/es-parser/issues"
  },
  "homepage": "https://github.com/Mister-Hope/es-parser",
  "dependencies": {
    "acorn": "^8.0.4",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.3",
    "webpack": "^5.2.0",
    "webpack-cli": "^4.1.0"
  },
  "devDependencies": {
    "@types/chai": "4.2.14",
    "@types/estree": "0.0.45",
    "@types/mocha": "8.0.3",
    "@types/node": "14.14.2",
    "@types/webpack": "4.41.23",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "chai": "4.2.0",
    "conventional-changelog-cli": "2.1.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.12.0",
    "eslint-config-prettier": "6.14.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.0",
    "lint-staged": "10.4.2",
    "mocha": "8.2.0",
    "prettier": "2.1.2",
    "ts-node": "9.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run build && git add dist/parser.js && git add dist/parser.js.map && lint-staged"
    }
  }
}
