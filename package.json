{
  "name": "es-parser",
  "version": "0.0.1",
  "description": "A mini es parser",
  "main": "dist/parser.js",
  "types": "dist/parser.d.ts",
  "scripts": {
    "dev": "webpack --watch",
    "build": "webpack",
    "test": "mocha -r ts-node/register test/unit/**/*.ts",
    "test:parser": "ts-node ./test/parser.ts",
    "test:dev": "mocha -r ts-node/register  --watch test/unit/**/*.ts",
    "test:js": "mocha ./test/unit/index.js",
    "parse": "node ./test/parse.js",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "commit": "npx git-cz"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mister-Hope/es-parser.git"
  },
  "keywords": [
    "javascript",
    "parser",
    "mr-hope"
  ],
  "author": "Mr.Hope",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mister-Hope/es-parser/issues"
  },
  "homepage": "https://github.com/Mister-Hope/es-parser",
  "dependencies": {
    "acorn": "^7.1.1",
    "ts-loader": "^3.5.0",
    "typescript": "^3.8.3",
    "webpack": "^3.11.0"
  },
  "devDependencies": {
    "@types/chai": "4.2.11",
    "@types/estree": "0.0.42",
    "@types/mocha": "7.0.2",
    "@types/node": "13.11.0",
    "@typescript-eslint/eslint-plugin": "2.26.0",
    "@typescript-eslint/parser": "2.26.0",
    "chai": "4.2.0",
    "conventional-changelog-cli": "2.0.31",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.3",
    "lint-staged": "10.1.2",
    "mocha": "7.1.1",
    "prettier": "2.0.4",
    "ts-node": "8.8.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
