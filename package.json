{
  "name": "es-parser",
  "version": "0.0.1",
  "description": "A mini es parser",
  "main": "dist/parser.js",
  "types": "dist/parser.d.ts",
  "scripts": {
    "dev": "webpack --watch",
    "build": "webpack",
    "test": "mocha -r ts-node/register test/unit/**/*.ts",
    "test:parser": "ts-node ./test/parser.ts",
    "test:dev": "mocha -r ts-node/register  --watch test/unit/**/*.ts",
    "test:js": "mocha ./test/unit/index.js",
    "parse": "node ./test/parse.js",
    "debug": "mocha -r ts-node/register  --watch test/debug.ts",
    "lint": "yarn run lint:check --fix",
    "lint:check": "eslint --ext .js,.ts .",
    "commit": "npx git-cz"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mister-Hope/es-parser.git"
  },
  "keywords": [
    "javascript",
    "parser",
    "mr-hope"
  ],
  "author": "Mr.Hope",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mister-Hope/es-parser/issues"
  },
  "homepage": "https://github.com/Mister-Hope/es-parser",
  "dependencies": {
    "acorn": "^7.1.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "@types/chai": "4.2.11",
    "@types/estree": "0.0.42",
    "@types/mocha": "7.0.2",
    "@types/node": "13.9.1",
    "@types/webpack": "4.41.7",
    "@typescript-eslint/eslint-plugin": "2.23.0",
    "@typescript-eslint/parser": "2.23.0",
    "chai": "4.2.0",
    "conventional-changelog-cli": "2.0.31",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "mocha": "7.1.0",
    "prettier": "1.19.1",
    "ts-node": "8.6.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
