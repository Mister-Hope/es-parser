{
  "name": "es-parser",
  "version": "0.0.1",
  "description": "A mini es parser",
  "keywords": [
    "javascript",
    "parser",
    "mr-hope"
  ],
  "homepage": "https://github.com/Mister-Hope/es-parser",
  "bugs": {
    "url": "https://github.com/Mister-Hope/es-parser/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mister-Hope/es-parser.git"
  },
  "license": "MIT",
  "author": "Mr.Hope",
  "main": "dist/parser.js",
  "types": "dist/parser.d.ts",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "commit": "yarn git-cz",
    "dev": "rollup -c rollup.config.js -w",
    "lint": "prettier --check --write . && eslint --fix --ext .js,.ts .",
    "lint:check": "prettier --check . && eslint --ext .js,.ts .",
    "parse": "node ./test/parse.js",
    "prepare": "husky install",
    "test": "jest -i",
    "test:debug": "mocha -r ts-node/register  --watch test/debug.ts",
    "test:dev": "mocha -r ts-node/register  --watch test/unit/**/*.ts",
    "test:js": "mocha ./test/unit/index.js",
    "test:parser": "ts-node ./test/parser.ts"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,json,yml}": [
      "prettier --write"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "8.2.1",
    "@types/estree": "0.0.49",
    "@types/jest": "26.0.23",
    "@types/node": "18.14.0",
    "@types/webpack": "5.28.0",
    "@typescript-eslint/eslint-plugin": "4.28.1",
    "@typescript-eslint/parser": "4.33.0",
    "acorn": "8.4.1",
    "babylon": "^6.18.0",
    "conventional-changelog-cli": "2.1.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "8.0.1",
    "jest": "27.0.6",
    "lint-staged": "11.0.0",
    "prettier": "2.6.2",
    "rollup": "2.75.3",
    "rollup-plugin-dts": "3.0.2",
    "rollup-plugin-terser": "7.0.2",
    "sort-package-json": "1.50.0",
    "ts-jest": "27.0.3",
    "ts-node": "10.0.0",
    "typescript": "4.3.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
